def add (n: ChurchInt) (m: ChurchInt) = n succ m
//│ add: ChurchInt -> (forall 'b. (ChurchInt -> (forall 'c, 'a, 'd, 'a0, 'e, 'f, 'g, 'h. ((('d | 'f) -> 'g & 'e & 'h) -> (forall 'd, 'a0, 'f, 'g, 'h, 'i, 'j, 'k, 'l. (('d & 'a0 & 'j) -> ('l | 'g)
