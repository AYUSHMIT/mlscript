// let nfind table key =
//   begin match find table key with
//   | None -> fun f x -> x
//   | Some n -> n
//   end
def nfind table key =
  let opt = find table key in
  case opt of
  { None -> fun f -> fun x -> x
  | Some -> opt.val
  }
//│ nfind: List[?] -> (forall 'val, 'val0. (anything -> (anything -> (forall 'a. 'a -> 'a) | 'val0 | 'val)
//│   where
//│     'val <: 'val0))
//│      = [Function: nfind]

