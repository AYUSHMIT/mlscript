// let succ (n:Int) = fun f x -> f (n f x)
def succ (n: ChurchInt) = fun f -> fun x -> f (n f x)
def succ_ n = fun f -> fun x -> f (n f x)
//│ succ: ChurchInt -> (forall 'b, 'a, 'c, 'a0, 'd, 'e, 'f, 'g. ((('c | 'e) -> 'f & 'd & 'g) -> (forall 'c, 'a0, 'e, 'f, 'g, 'h, 'i, 'j, 'k. (('c & 'a0 & 'i) -> ('k | 'f)
