// let nfind table key =
//   begin match find table key with
//   | None -> fun f x -> x
//   | Some n -> n
//   end
def nfind table key =
  let opt = find table key in
  case opt of
  { None -> fun f -> fun x -> x
  | Some -> opt.val
  }
//│ nfind: (List['a & {_1: 'b, _2: 'c} & 'a0 & 'a1 & 'a2 & 'a3 | 'a4 | {_1: 'd} & 'a5 | {_2: 'e} & 'a6] & 'f) -> (forall 'g, 'val. 'g -> (forall 'h. 'h -> (forall 'i. 'i -> 'i) | 'val | 'c | 'e))
//│      = [Function: nfind]

// type Int = ['a] ('a -> 'a) -> ('a -> 'a)
type ChurchInt = forall 'a. ('a -> 'a) -> ('a -> 'a)
//│ Defined type alias ChurchInt

// let succ (n:Int) = fun f x -> f (n f x)
def succ (n: ChurchInt) = fun f -> fun x -> f (n f x)
def succ_ n = fun f -> fun x -> f (n f x)
//│ succ: ChurchInt -> (forall 'b, 'c, 'a, 'a0, 'd. (('a0 | 'b) -> 'd & 'a -> ('a0 & 'a) & 'c) -> (forall 'e, 'a1, 'f, 'g. ('b & 'a & 'e & 'a1 & 'f) -> ('g | 'd)))
//│     = [Function: succ]
//│ succ_: ('a -> ('b -> 'c & 'd) & 'e) -> (forall 'f, 'g, 'h. (('c | 'f) -> 'g & 'a & 'h) -> (forall 'i, 'j. ('b & 'i) -> ('j | 'g)))
//│      = [Function: succ_]

// let add (n:Int) (m:Int) = n succ m

//:e // due to tapping
def add (n: ChurchInt) (m: ChurchInt) = n succ m
//│ add: ChurchInt -> ChurchInt -> (forall 'b, 'a, 'c, 'a0, 'd, 'e. (('a | 'b) -> 'c & 'a0 -> ('a & 'a0) & 'd) -> (forall 'f, 'a1, 'g, 'h. ('b & 'a0 & 'f & 'a1 & 'g) -> ('h | 'c)) | ChurchInt | 'e)
//│    = [Function: add]

def add_ n m = n succ_ m
//│ add_: ((forall 'a, 'b, 'c, 'd, 'e. ('e -> ('c -> 'b & 'a) & 'd) -> (forall 'f, 'g, 'h. (('b | 'g) -> 'h & 'e & 'f) -> (forall 'i, 'j. ('c & 'i) -> ('j | 'h)))) -> ('k -> 'l & 'm) & 'n) -> (forall 'o, 'p. ('k & 'o) -> ('p | 'l))
//│     = [Function: add_]

