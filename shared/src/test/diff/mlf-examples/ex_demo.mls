//│   where
//│     'b <: (forall 'a. 'a -> 'a) -> 'c))
//│   where
//│     'b <: (forall 'a. 'a -> 'a) -> 'b))
//│       = [Function: c_true]

// let c_false = fun a b -> b c_unit
def c_false = fun a -> fun b -> b c_unit
//│ c_false: 'a -> ((forall 'b. 'b -> 'b) -> 'a & 'a) -> (forall 'a. ('c
//│   where
