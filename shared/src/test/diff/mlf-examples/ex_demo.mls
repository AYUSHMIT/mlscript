def c_add n (m: Fint) = m c_succ n
//│ c_add: (Fint & 'b) -> Fint -> (forall 'c, 'd. 'c -> (forall 'a, 'e, 'c, 'f, 'g. ('g -> 'e
//│   where
//│     'c <: 'a -> ('a & 'e) & 'g -> ('a & 'e & 'f))) | 'b | 'd)
