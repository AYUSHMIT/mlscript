// type sid = ['a] 'a -> 'a
type Sid = forall 'a. 'a -> 'a
//│ Defined type alias Sid

// let id = fun x -> x
// ;;
def id = fun x -> x
//│ id: 'a -> 'a
//│   = [Function: id]

// let delta = fun (x:sid) -> x x
// ;;
def delta = fun (x: Sid) -> x x
def delta' = fun x -> x x
//│ delta: Sid -> Sid
//│      = [Function: delta]
//│ delta': ('a -> 'b & 'a) -> 'b
//│       = [Function: delta]

// delta id ;;
delta id
delta' id
//│ res: Sid
//│    = [Function: id]
//│ res: 'a -> 'a
//│    = [Function: id]

// let t a b c d e = (fun x -> a x x) ((fun y -> b (c y)) (d e)) ;;
def t a b c d e = (fun x -> a x x) ((fun y -> b (c y)) (d e))
//│ t: (nothing -> nothing -> 'a & 'b) -> (forall 'a, 'b, 'c, 'd. ((nothing -> 'c & nothing -> 'a & 'd) -> (forall 'a, 'b, 'd, 'e, 'f, 'g, 'h. ((nothing -> 'g & nothing -> 'f & 'h) -> (forall 'a, 'b, 'd, 'h, 'i, 'j, 'k, 'l. ((nothing -> 'l & 'a) -> (forall 'a, 'b, 'd, 'h, 'm, 'n, 'o, 'p, 'q, 'r. ('r -> 'n
//│   where
//│     'b <: nothing -> nothing -> 'a & 'm -> 'm -> 'n
//│     'd <: 'o -> 'a & 'p -> 'm
//│     'h <: nothing -> 'o & 'q -> 'p
//│     'a <: 'r -> 'q))
//│   where
//│     'b <: nothing -> nothing -> 'a & 'i -> 'i -> 'a
//│     'd <: 'j -> 'a & 'k -> 'i
//│     'h <: nothing -> 'j & 'l -> 'k))
//│   where
//│     'b <: nothing -> nothing -> 'a & 'e -> 'e -> 'a
//│     'd <: 'f -> 'a & 'g -> 'e))
