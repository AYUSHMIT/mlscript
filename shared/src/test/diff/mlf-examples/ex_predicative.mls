// let t w z a b = (fun y -> (fun x -> w (x y) z) a) b ;;
def t w z a b = (fun y -> (fun x -> w (x y) z) a) b
//│ t: (nothing -> nothing -> 'a & 'b) -> (forall 'a, 'b, 'c. ('c -> (forall 'a, 'b, 'c, 'd, 'e. ((nothing -> 'e & nothing -> 'd & 'a) -> (forall 'a, 'b, 'c, 'f, 'g, 'h, 'i. ('i -> 'h
//│   where
//│     'b <: nothing -> 'c -> 'a & 'f -> 'c -> 'a & 'g -> 'c -> 'h
//│     'a <: nothing -> 'f & 'i -> 'g))
//│   where
//│     'b <: nothing -> 'c -> 'a & 'd -> 'c -> 'a & 'e -> 'c -> 'a))
//│   where
//│     'b <: nothing -> 'c -> 'a))
//│  = [Function: t1]

// let t y a = (fun x -> x (x y)) ((fun z -> z) (fun x -> a x)) ;;
def t y a = (fun x -> x (x y)) ((fun z -> z) (fun x -> a x))
