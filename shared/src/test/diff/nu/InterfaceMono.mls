trait Stadt {
    let name: string
}
//│ trait Stadt() {
//│   let name: string
//│ }

trait RefinedStadt extends Stadt {
    let size: int
    fun foo: bool -> int
}
//│ trait RefinedStadt() {
//│   fun foo: bool -> int
//│   let name: string
//│   let size: int
//│ }

trait SizedStadt extends RefinedStadt {
    let size: 1 | 2 | 3
    fun bar: int -> int
}
//│ trait SizedStadt() {
//│   fun bar: int -> int
//│   fun foo: bool -> int
//│   let name: string
//│   let size: 1 | 2 | 3
//│ }

class Goodstatt(size: 1 | 2) extends RefinedStadt {
    let name = "good"
    fun bar(x) = x
    fun foo(t) = if t && true then this.size else 0
}
//│ class Goodstatt(size: 1 | 2) {
//│   fun bar: forall 'a. 'a -> 'a
//│   fun foo: bool -> (0 | 1 | 2)
//│   let name: "good"
//│ }

