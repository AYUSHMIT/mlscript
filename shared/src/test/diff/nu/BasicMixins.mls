mixin BaseInc {
  fun test = super.base + 1
  fun test2 = this.base
}
//│ mixin BaseInc() {
//│   super: {base: int}
//│   this: {base: 'base}
//│   fun test: int
//│   fun test2: 'base
//│ }

// :d
class Base1(base: int): BaseTest, BaseInc {
  fun test3 = [base, this.base]
}
//│ class Base1(base: int) {
//│   fun test: int
//│   fun test2: int
//│   fun test3: (int, int,)
//│ }

Base1(1).test
//│ int

Base1(1).test2
//│ int

Base1(1).test3
//│ (int, int,)


