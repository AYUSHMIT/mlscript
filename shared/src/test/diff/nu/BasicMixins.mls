mixin WrapBase {
  // fun wrap(x) = x
  // fun wrap(x) = x : int
  fun wrapA(x: int) = x : int
  fun wrap(x) = x
}
//│ mixin WrapBase() {
//│   fun wrap: 'a -> 'a
//│   fun wrapA: (x: int,) -> int
//│ }

// :d
mixin Wrap {
  fun wrapA(x) = [super.wrapA(x)]
  fun wrap(x) = [super.wrap(x)]
}
//│ mixin Wrap() {
//│   super: {wrap: 'a -> 'b, wrapA: 'c -> 'd}
//│   fun wrap: 'a -> ('b,)
//│   fun wrapA: 'c -> ('d,)
//│ }



// :d
module WrapBase1: WrapBase, Wrap
