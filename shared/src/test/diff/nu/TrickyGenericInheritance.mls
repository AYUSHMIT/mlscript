trait T1[A] {
  fun f: A -> A
}
//│ trait T1[A] {
//│   fun f: A -> A
//│ }

class C1 extends T1 {
  fun f(x: int) = x
}
//│ class C1 {
//│   fun f: (x: int,) -> int
//│ }

class C1 extends T1['FigureItOut] {
  fun f(x: int) = x
}
//│ class C1 {
//│   fun f: (x: int,) -> int
//│ }

let c1 = new C1
//│ let c1: C1

c1.f
//│ (x: int,) -> int

(c1 : T1).f
//│ (??A & 'A) -> ('A | ??A0)

(c1 : T1['X]).f
//│ int -> int

:ns
(c1 : T1).f
