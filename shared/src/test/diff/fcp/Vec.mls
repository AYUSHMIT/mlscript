type AnyVec[A] = forall 'r. (forall 'p. Nil | Cons[A, S['p]] -> 'r) -> 'r
//│ Defined type alias AnyVec[+A]

def Nil = Nil { size = Z{} }
def Cons head tail = Cons { head; tail; size = S{} }
//│ Nil: Nil
//│ Cons: 'a -> (Vec['A, 'N] & 'tail) -> (Cons['A | 'a, 'N] with {head: 'a, size: forall 'P. S['P], tail: 'tail})


