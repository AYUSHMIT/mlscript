
:GeneralizeCurriedFunctions

// def foo f x = f x


// def s n = fun f -> fun x -> f (n f x)



def z f x = x
//│ z: anything -> (forall 'a. 'a -> 'a)
//│  = [Function: z]

def s n = fun f -> fun x -> f (n f x)
//│ s: 'a -> (forall 'b. 'b -> (forall 'a, 'b, 'c, 'd, 'e. ('c -> 'e
//│   where
//│     'a <: 'b -> 'c -> 'd
//│     'b <: 'd -> 'e)))
//│  = [Function: s]

// :d
n1 = s z
//│ n1: 'a -> (forall 'a, 'b, 'c, 'd, 'e. ('c -> 'e
//│   where
//│     'b <: 'a -> 'c -> 'd
//│     'a <: 'd -> 'e))
//│   = [Function (anonymous)]

n2 = s (s z)
//│ n2: 'a -> (forall 'a, 'b, 'c, 'd, 'e. ('c -> 'e
//│   where
//│     'b <: 'a -> 'c -> 'd
//│     'a <: 'd -> 'e))
//│   = [Function (anonymous)]

n3 = s (s (s z))
//│ n3: 'a -> (forall 'a, 'b, 'c, 'd, 'e. ('c -> 'e
//│   where
//│     'b <: 'a -> 'c -> 'd
//│     'a <: 'd -> 'e))
//│   = [Function (anonymous)]

// def wrap: 'WRA -> 'WRA
// def wrap: int -> {x:int}

// def wrap: 'a -> {x: 'a}
wrap x = { x }
//│ wrap: 'a -> {x: 'a}
//│     = [Function: wrap]

// :d
n1w = n1 wrap
//│ n1w: 'a -> 'b
//│   where
//│     'c <: 'd -> 'a -> 'e
//│     'd <: 'e -> 'b
//│    = [Function (anonymous)]

// :d
n1w 0
//│ res: {x: 0}
//│    = { x: 0 }

n1 wrap 0
//│ res: {x: 0}
//│    = { x: 0 }

n2 wrap 0
//│ res: {x: {x: 0}}
//│    = { x: { x: 0 } }

n3 wrap 0
//│ res: {x: {x: {x: 0}}}
//│    = { x: { x: { x: 0 } } }



