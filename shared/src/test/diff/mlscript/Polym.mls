
// :d
// :s
def foo f =
  f (fun x -> x)
//│ foo: ((forall 'a. 'a -> 'a) -> 'b) -> 'b
//│    = [Function: foo]

def bar h =
  (h 0, h true)
//│ bar: ((0 | true) -> 'a) -> ('a, 'a,)
//│    = [Function: bar]

bar id
//│ res: (0, true,)
//│    = [ 0, true ]

foo bar
//│ res: (0, true,)
//│    = [ 0, true ]


def foo2 f =
  f id
//│ foo2: ((forall 'a. 'a -> 'a) -> 'b) -> 'b
//│     = [Function: foo2]

foo2 bar
//│ res: (0, true,)
//│    = [ 0, true ]



