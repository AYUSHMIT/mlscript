// (* A function that receives a token *)
def f t = (s x x t x x e)
//│ f: (() -> List["x"] -> (() -> (forall 'b, 'c. (List['b] & 'c) -> (forall 'c, 'a, 'd, 'e. ((() -> List['a] -> 'e) -> 'e
//│   where
//│     'c <: List['a]
//│     'd <: 'a)))) -> (() -> (forall 'b, 'c. (List['b] & 'c) -> (forall 'c, 'a, 'd, 'e. ((() -> List['a] -> 'e) -> 'e
//│   where
//│     'c <: List['a]
//│     'd <: 'a)))) -> (() -> List[string] -> () -> (forall 'b. (List['b] & 'b) -> (forall 'b, 'a0, 'f, 'g. ((() -> List['a0] -> 'g) -> 'g
//│   where
//│     'b <: List['a0]
//│     'f <: 'a0)))) -> 'h) -> 'h

// (* If the token is used twice, we must reveive two arguments *)
def g t1 t2 = (s x x t1 x (s x t2 x e) e)
//│ g: (() -> List["x"] -> (() -> (forall 'b, 'c. (List['b] & 'c) -> (forall 'c, 'a, 'd, 'e. ((() -> List['a] -> 'e) -> 'e
//│   where
//│     'c <: List['a]
//│     'd <: 'a)))) -> nothing -> (() -> List[string] -> () -> (forall 'b. (List['b] & 'b) -> (forall 'b, 'a0, 'f, 'g. ((() -> List['a0] -> 'g) -> 'g
//│   where
//│     'b <: List['a0]
//│     'f <: 'a0)))) -> 'b & 'h) -> (forall 'h. ((() -> List["x"] -> (() -> (forall 'b, 'c. (List['b] & 'c) -> (forall 'c, 'a, 'd, 'e. ((() -> List['a] -> 'e) -> 'e
//│   where
//│     'c <: List['a]
//│     'd <: 'a)))) -> (() -> List[string] -> () -> (forall 'b. (List['b] & 'b) -> (forall 'b, 'a0, 'f, 'g. ((() -> List['a0] -> 'g) -> 'g
//│   where
//│     'b <: List['a0]
//│     'f <: 'a0)))) -> 'i) -> 'j
//│   where
//│     'h <: () -> List["x"] -> (() -> (forall 'b, 'c. (List['b] & 'c) -> (forall 'c, 'a, 'd, 'e. ((() -> List['a] -> 'e) -> 'e
//│   where
//│     'c <: List['a]
//│     'd <: 'a)))) -> 'i -> (() -> List[string] -> () -> (forall 'b. (List['b] & 'b) -> (forall 'b, 'a0, 'f, 'g. ((() -> List['a0] -> 'g) -> 'g
//│   where
//│     'b <: List['a0]
//│     'f <: 'a0)))) -> 'j))

